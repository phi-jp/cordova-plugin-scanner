<?xml version='1.0' encoding='utf-8'?>
<widget id="cordova.plugin.test.scanner" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>cordova-plugin-scanner-test</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <content src="http://localhost:8080/cordova-plugin-scanner/test/www/index.html" />
    <content src="http://sp.local:8080/cordova-plugin-scanner/test/www/index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1.3.3" />
    <access origin="*" />
    <allow-navigation href="http://localhost:8080/*" />
    <allow-navigation href="http://sp.local:8080/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform> 
</widget>
